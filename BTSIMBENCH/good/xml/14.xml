<Sequence class="SequenceNode" instance_name="Collect_Blackberries_Garden">
    <Sequence class="SequenceNode" instance_name="Navigate_Pick_Blackberries">        
        <Fallback class="FallbackNode" instance_name="Move_to_scrub">
            <Condition class="robot_is_near_scrub?" instance_name="robot_is_near_scrub?"></Condition>
            <Action class="Move_to_scrub" instance_name="Move_to_scrub"></Action>
        </Fallback>
        <Fallback class="FallbackNode" instance_name="Detect_blackberry">
            <Condition class="blackberry_is_known?" instance_name="blackberry_is_known?"></Condition>
            <Action class="Detect_blackberry" instance_name="Detect_blackberry"></Action>
        </Fallback>        
        <Fallback class="FallbackNode" instance_name="Pick_blackberry">
            <Condition class="blackberry_in_gripper?" instance_name="blackberry_in_gripper?"></Condition>
            <Sequence class="SequenceNode" instance_name="Pick_blackberry">
