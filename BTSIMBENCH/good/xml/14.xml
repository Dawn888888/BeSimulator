                    <Condition class="robot_is_near_blackberry?" instance_name="robot_is_near_blackberry?"></Condition>
                    <Action class="Move_to_blackberry" instance_name="Move_to_blackberry"></Action>
                </Fallback>
                <Action class="Pick_blackberry" instance_name="Pick_blackberry"></Action>
            </Sequence>
        </Fallback>
    </Sequence>
    <Sequence class="SequenceNode" instance_name="Deposit_Berries_Basket">
        <Fallback class="FallbackNode" instance_name="Move_to_basket">
            <Condition class="robot_is_near_basket?" instance_name="robot_is_near_basket?"></Condition>
            <Action class="Move_to_basket" instance_name="Move_to_basket"></Action>
        </Fallback>  
        <Action class="Deposit_blackberry" instance_name="Deposit_blackberry"></Action>
    </Sequence>
</Sequence>
